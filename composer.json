{
    "name": "melvinachterhuis/checkout-header-footer-shopware6-plugin",
    "description": "Full header and footer in the checkout",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Melvin Achterhuis"
        }
    ],
    "require": {
